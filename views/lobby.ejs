<link href="https://fonts.googleapis.com/css2?family=Orbitron:wght@400;700;900&family=Rajdhani:wght@400;600;700&display=swap" rel="stylesheet">
<style>
    .lobby-container {
        max-width: 900px;
        margin: 30px auto;
        padding: 20px;
        font-family: 'Rajdhani', sans-serif;
    }
    
    /* Player Card */
    .player-card {
        background: linear-gradient(135deg, #1a1a2e 0%, #16213e 100%);
        border: 3px solid #444;
        border-radius: 20px;
        padding: 30px;
        margin-bottom: 30px;
        box-shadow: 0 10px 40px rgba(0,0,0,0.5);
        position: relative;
        overflow: hidden;
    }
    
    .player-card::before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        height: 5px;
        background: linear-gradient(90deg, #e74c3c, #f39c12, #e74c3c);
    }
    
    .player-info {
        display: flex;
        align-items: center;
        gap: 20px;
    }
    
    .player-avatar {
        width: 80px;
        height: 80px;
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        font-size: 2.5em;
        border: 4px solid;
        box-shadow: 0 0 20px rgba(255,255,255,0.2);
    }
    
    .player-details h2 {
        margin: 0 0 5px 0;
        font-family: 'Orbitron', sans-serif;
        font-weight: 700;
        font-size: 1.8em;
        text-shadow: 2px 2px 4px rgba(0,0,0,0.5);
    }
    
    .player-details p {
        margin: 0;
        color: #888;
        font-size: 0.9em;
    }
    
    .player-details p code {
        background: rgba(255,255,255,0.1);
        padding: 2px 8px;
        border-radius: 4px;
        font-size: 0.85em;
    }
    
    /* Menu Grid */
    .menu-grid {
        display: grid;
        grid-template-columns: repeat(2, 1fr);
        gap: 20px;
        margin-bottom: 30px;
    }
    
    .menu-item {
        background: linear-gradient(135deg, #1e1e1e 0%, #2d2d2d 100%);
        border: 3px solid #444;
        border-radius: 16px;
        padding: 30px 20px;
        text-align: center;
        cursor: pointer;
        transition: all 0.3s ease;
        text-decoration: none;
        position: relative;
        overflow: hidden;
    }
    
    .menu-item::before {
        content: '';
        position: absolute;
        top: 0;
        left: -100%;
        width: 100%;
        height: 100%;
        background: linear-gradient(90deg, transparent, rgba(255,255,255,0.1), transparent);
        transition: left 0.5s;
    }
    
    .menu-item:hover::before {
        left: 100%;
    }
    
    .menu-item:hover {
        transform: translateY(-5px);
        box-shadow: 0 10px 30px rgba(0,0,0,0.5);
    }
    
    .menu-item.play {
        border-color: #e74c3c;
        grid-column: span 2;
    }
    
    .menu-item.play:hover {
        border-color: #ff6b6b;
        box-shadow: 0 10px 30px rgba(231, 76, 60, 0.4);
    }
    
    .menu-item.profile {
        border-color: #3498db;
    }
    
    .menu-item.profile:hover {
        border-color: #5dade2;
        box-shadow: 0 10px 30px rgba(52, 152, 219, 0.4);
    }
    
    .menu-item.settings {
        border-color: #f39c12;
    }
    
    .menu-item.settings:hover {
        border-color: #f1c40f;
        box-shadow: 0 10px 30px rgba(243, 156, 18, 0.4);
    }
    
    .menu-item.howto {
        border-color: #9b59b6;
        grid-column: span 2;
    }
    
    .menu-item.howto:hover {
        border-color: #a569bd;
        box-shadow: 0 10px 30px rgba(155, 89, 182, 0.4);
    }
    
    .menu-item.howto .menu-icon {
        font-size: 4em;
    }
    
    .menu-item.howto .menu-title {
        font-size: 1.8em;
    }
    
    .menu-icon {
        font-size: 3em;
        margin-bottom: 15px;
        display: block;
    }
    
    .menu-item.play .menu-icon {
        font-size: 4em;
    }
    
    .menu-title {
        font-family: 'Orbitron', sans-serif;
        font-weight: 700;
        font-size: 1.4em;
        color: #fff;
        margin-bottom: 8px;
    }
    
    .menu-item.play .menu-title {
        font-size: 1.8em;
    }
    
    .menu-desc {
        color: #888;
        font-size: 0.95em;
    }
    
    /* Quick Stats */
    .quick-stats {
        display: flex;
        justify-content: center;
        gap: 30px;
        margin-top: 20px;
        padding-top: 20px;
        border-top: 2px solid #333;
    }
    
    .quick-stat {
        text-align: center;
    }
    
    .quick-stat-value {
        font-family: 'Orbitron', sans-serif;
        font-size: 1.5em;
        font-weight: 700;
        color: #fff;
    }
    
    .quick-stat-label {
        color: #666;
        font-size: 0.85em;
    }
    
    /* Bottom Links */
    .bottom-links {
        display: flex;
        justify-content: center;
        gap: 15px;
        flex-wrap: wrap;
    }
    
    .bottom-link {
        padding: 12px 24px;
        background: #333;
        border: 2px solid #444;
        border-radius: 8px;
        color: #aaa;
        text-decoration: none;
        font-weight: 600;
        transition: all 0.3s;
    }
    
    .bottom-link:hover {
        background: #444;
        color: #fff;
        border-color: #555;
    }
    
    .bottom-link.admin {
        border-color: #9b59b6;
        color: #9b59b6;
    }
    
    .bottom-link.admin:hover {
        background: #9b59b6;
        color: #fff;
    }

    @media (max-width: 600px) {
        .menu-grid {
            grid-template-columns: 1fr;
        }
        
        .menu-item.play {
            grid-column: span 1;
        }
        
        .player-info {
            flex-direction: column;
            text-align: center;
        }
        
        .quick-stats {
            flex-wrap: wrap;
        }
    }
</style>

